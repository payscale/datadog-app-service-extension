<?xml version="1.0"?>
<package>
  <metadata>
    <id>DatadogAPM.AzureAppService.Extension</id>
    <version>1.3.1</version>
    <title>.NET Core Datadog APM x86</title>
    <authors>Robert Glickman</authors>
    <owners>Robert Glickman</owners>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Datadog APM installer for a .Net Core non-self-contained app. Requires a host accessible from the App Service running the Datadog agent. Add Configuration parameters DD_AGENT_HOST, DD_API_KEY, and (optionally) DD_ENV and DD_SERVICE_NAME
    </description>
    <summary>Datadog APM installer for a .Net Core non-self-contained app</summary>
    <tags>AzureSiteExtension Datadog .net apm</tags>
    <packageTypes>
      <packageType name="AzureSiteExtension" />
    </packageTypes>
  </metadata>
  <files>
    <file src="content\**\*.*" target="content" />
  </files>
</package>